fo, [name]: value });\r\n    //   };\r\n    // console.log(info);\r\n    return (\r\n        <div className='container-login'>\r\n            <div className='row my-4'>\r\n                <div>\r\n                    <div className='w-100'>\r\n                        <img className='login-img' src='/img/cover-1.jpg' alt='123' />\r\n                    </div>\r\n                </div>\r\n                <div className='login-wrapform'>\r\n                    <form\r\n                        onSubmit={handleSubmit(onSubmit)}\r\n                        className='login-form'>\r\n                        <h3 className='text-center mb-4'>\r\n                            {' '}\r\n                            <span>WELCOME</span> TO SIGN IN\r\n                        </h3>\r\n                        <div className='email'>\r\n                            <label>Email</label>\r\n                            <input\r\n                                type='text'\r\n                                onChange={handleChangeInput}\r\n                                name='email'\r\n                                value={email}\r\n                                className='form-control'\r\n                                ref={register}\r\n                            />\r\n                            <p className='err-message'>{errors.email?.message}</p>\r\n                        </div>\r\n                        <div className='password'>\r\n                            <label>Password</label>\r\n                            <input\r\n                                type='password'\r\n                                onChange={handleChangeInput}\r\n                                name='password'\r\n                                value={password}\r\n                                className='form-control'\r\n                                ref={register}\r\n                            />\r\n                            <p className='err-message'>{errors.password?.message}</p>\r\n            